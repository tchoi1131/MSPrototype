{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MSPrototypeDataFactory"
		},
		"MSPrototypeDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'MSPrototypeDataLakeStorageGen2'"
		},
		"MSPrototypeSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MSPrototypeSqlServer'"
		},
		"MSPrototypeSqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MSPrototypeSqlServer'"
		},
		"MSPrototypeDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tomwong.dfs.core.windows.net"
		},
		"MSPrototypeSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "REDMOND\\v-shli5"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypePipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "MSPrototypeCopyData",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SqlServerSource",
								"sqlReaderQuery": "SELECT [EventId]\n      ,[EventDate]\n      ,[EventType]\n      ,[WorkFlowId]\n      ,[WorkFlowName]\n      ,[InsertedDateTime]\n      ,[UpdatedDateTime]\n      ,[IsDeleted]\n      ,[IsTestData]\n  FROM [vwWorkFlowEvent] WITH (NOLOCK)",
								"queryTimeout": "02:00:00"
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings",
									"maxConcurrentConnections": 30
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "MSPrototypeSqlServerVWWorkFlowEvent",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "MSPrototypeDelimitedText",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/MSPrototypeSqlServerVWWorkFlowEvent')]",
				"[concat(variables('factoryId'), '/datasets/MSPrototypeDelimitedText')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypeDelimitedText')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "MSPrototypeDataLakeStorageGen2",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@concat('Workflow_', formatDateTime(utcnow(), 'MM-dd-yyyy'))",
							"type": "Expression"
						},
						"folderPath": "WorkFlow",
						"fileSystem": "msprototype"
					},
					"columnDelimiter": "|",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/MSPrototypeDataLakeStorageGen2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypeSqlServerVWWorkFlowEvent')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "MSPrototypeSqlServer",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [
					{
						"name": "EventId",
						"type": "uniqueidentifier"
					},
					{
						"name": "EventDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "EventType",
						"type": "varchar"
					},
					{
						"name": "WorkFlowId",
						"type": "smallint",
						"precision": 5
					},
					{
						"name": "WorkFlowName",
						"type": "varchar"
					},
					{
						"name": "InsertedDateTime",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "UpdatedDateTime",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "IsDeleted",
						"type": "int",
						"precision": 10
					},
					{
						"name": "IsTestData",
						"type": "int",
						"precision": 10
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "vwWorkFlowEvent"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/MSPrototypeSqlServer')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypeDataLakeStorageGen2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('MSPrototypeDataLakeStorageGen2_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('MSPrototypeDataLakeStorageGen2_accountKey')]"
					}
				},
				"connectVia": {
					"referenceName": "MSPrototypeIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/MSPrototypeIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypeSqlServer')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('MSPrototypeSqlServer_connectionString')]",
					"userName": "[parameters('MSPrototypeSqlServer_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('MSPrototypeSqlServer_password')]"
					}
				},
				"connectVia": {
					"referenceName": "MSPrototypeIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/MSPrototypeIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MSPrototypeIntegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}