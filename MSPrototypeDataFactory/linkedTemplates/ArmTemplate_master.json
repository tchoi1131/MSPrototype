{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MSPrototypeDataFactory"
		},
		"MSPrototypeDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'MSPrototypeDataLakeStorageGen2'"
		},
		"MSPrototypeSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MSPrototypeSqlServer'"
		},
		"MSPrototypeSqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MSPrototypeSqlServer'"
		},
		"MSPrototypeDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tomwong.dfs.core.windows.net"
		},
		"MSPrototypeSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "REDMOND\\v-shli5"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MSPrototypeDataLakeStorageGen2_accountKey": {
						"value": "[parameters('MSPrototypeDataLakeStorageGen2_accountKey')]"
					},
					"MSPrototypeSqlServer_connectionString": {
						"value": "[parameters('MSPrototypeSqlServer_connectionString')]"
					},
					"MSPrototypeSqlServer_password": {
						"value": "[parameters('MSPrototypeSqlServer_password')]"
					},
					"MSPrototypeDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('MSPrototypeDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"MSPrototypeSqlServer_properties_typeProperties_userName": {
						"value": "[parameters('MSPrototypeSqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}